<h1>Carpooling</h1>
<h3>Locations:</h3>
<% @locations.each do |location|%>
<ul>
  <li><%= link_to "#{location.name}", location_path(location) %></li>
  <% end %>
</ul>

<h3>Routes:</h3>
<% @routes.each do |route|%>
<ul>
  <li><%= link_to "#{route.name}", route_path(route) %></li>
  <% end %>
</ul>

